<Window x:Class="eLogistics.Executable.Windows.AddressEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Adresas" Height="300" Width="461" ResizeMode="CanMinimize">
    <Grid>
        <TextBox Name="textName" HorizontalAlignment="Left" Height="23" Margin="147,19,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="296" Text="{Binding Path=Name}"/>

        <Label Content="Valstybės pavadinimas:" HorizontalAlignment="Left" Margin="10,19,0,0" VerticalAlignment="Top"/>

        <Button HorizontalAlignment="Left" Margin="288,240,0,0" VerticalAlignment="Top" Width="75" Content="Ok" Click="OnButtonOkClick"/>
        <Button HorizontalAlignment="Left" Margin="368,240,0,0" VerticalAlignment="Top" Width="75" Content="Cancel" Click="OnButtonCancelClick"/>
    </Grid>
</Window>
